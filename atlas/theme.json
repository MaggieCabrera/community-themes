				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--base)"
				},
				":hover": {
					"shadow": "2px 2px 0 0 var(--wp--preset--color--base), 4px 4px 0 0 var(--wp--preset--color--contrast)",
					"color": {
						"background": "var(--wp--preset--color--primary)",
						"text": "var(--wp--preset--color--base)"
					}
				},
				":focus": {
					"shadow": "2px 2px 0 0 var(--wp--preset--color--base), 4px 4px 0 0 var(--wp--preset--color--contrast)",
