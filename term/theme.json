{
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#131B1E",
					"name": "Black",
					"slug": "custom-black"
				},
				{
					"color": "#5F6669",
					"name": "Grey",
					"slug": "custom-grey"
				},
				{
					"color": "#EDECE8",
					"name": "Beige",
					"slug": "custom-beige"
				}
			]
		},
		"custom": {
			"fontWeight": {
				"black": 900,
				"bold": 700,
				"extra-bold": 800,
				"extra-light": 200,
				"light": 300,
				"medium": 500,
				"regular": 425,
				"semi-bold": 600,
				"thin": 100
			},
			"lineHeight": {
				"body": 1.5,
				"none": 1,
				"relaxed": 1.3999999999999999,
				"snug": 1.2,
				"tight": 1.1000000000000001
			}
		},
		"layout": {
			"contentSize": "800px",
			"wideSize": "1760px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"defaultFontSizes": false,
			"fontFamilies": [
				{
					"fontFamily": "'HelveticaNeue Regular','HelveticaNeue-Light', 'Helvetica Neue Light', 'HelveticaNeue', Helvetica, Arial, 'Lucida Grande', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				}
			],
			"fontSizes": [
				{
					"size": "1rem",
					"slug": "xx-small"
				},
				{
					"size": "1.25rem",
					"slug": "x-small"
				},
				{
					"size": "1.375rem",
					"slug": "small"
				},
				{
					"size": "1.5rem",
					"slug": "medium"
				},
				{
					"size": "1.75rem",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "2.375rem",
						"min": "2rem"
					},
					"size": "2.375rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"max": "3rem",
						"min": "2.6255rem"
					},
					"size": "3rem",
					"slug": "xx-large"
				},
				{
					"fluid": {
						"max": "4rem",
						"min": "3.25rem"
					},
					"size": "4rem",
					"slug": "huge"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/navigation": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "undeline"
							}
						}
					}
				},
				"spacing": {
					"blockGap": "var(--wp--preset--spacing--80)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"core/paragraph": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"core/site-tagline": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-small)"
				}
			},
			"core/site-title": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--custom-beige)",
			"text": "var(--wp--preset--color--black)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--huge)",
					"fontWeight": "var(--wp--custom--font-weight--bold)",
					"lineHeight": "var(--wp--custom--line-height--tight)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"fontWeight": "var(--wp--custom--font-weight--bold)",
					"lineHeight": "var(--wp--custom--line-height--snug)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "var(--wp--custom--font-weight--bold)",
					"lineHeight": "var(--wp--custom--line-height--snug)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "var(--wp--custom--font-weight--bold)",
					"lineHeight": "var(--wp--custom--line-height--relaxed)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "var(--wp--custom--font-weight--bold)",
					"lineHeight": "var(--wp--custom--line-height--relaxed)",
					"textTransform": "uppercase"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-small)",
					"fontWeight": "var(--wp--custom--font-weight--bold)",
					"lineHeight": "var(--wp--custom--line-height--snug)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--custom-grey)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--custom-black)"
					}
				},
				":active": { 
					"color": {
						"text": "var(--wp--preset--color--custom-black)"
					}
				},
				":focus": {
					"typography": {
						"textDecoration": "none"
					},
					"color": {
						"text": "var(--wp--preset--color--custom-black)"
					},
					"outline": {
						"style": "dotted",
						"color": "var(--wp--preset--color--custom-black)"
					}
				}
			}
		},
		"spacing": {
			"blockGap": "0",
			"padding": {
				"bottom": "0px",
				"left": "20px",
				"right": "20px",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system-font)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "var(--wp--custom--line-height--body)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.6/theme.json"
}