{
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#131B1E",
					"name": "Black",
					"slug": "custom-black"
				},
				{
					"color": "#5F6669",
					"name": "Grey",
					"slug": "custom-grey"
				},
				{
					"color": "#EDECE8",
					"name": "Beige",
					"slug": "custom-beige"
				}
			]
		},
		"layout": {
			"contentSize": "800px",
			"wideSize": "1800px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "'HelveticaNeue Regular','HelveticaNeue-Light', 'Helvetica Neue Light', 'HelveticaNeue', Helvetica, Arial, 'Lucida Grande', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				}
			],
			"fontSizes": [
				{
					"size": "1rem",
					"slug": "xx-small"
				},
				{
					"size": "1.25rem",
					"slug": "x-small"
				},
				{
					"size": "1.375rem",
					"slug": "small"
				},
				{
					"size": "1.5rem",
					"slug": "medium"
				},
				{
					"size": "1.75rem",
					"slug": "large"
				},
				{
					"fluid": {
						"min": "2rem",
						"max": "2.375rem"
					},
					"size": "2.375rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "2.6255rem",
						"max": "3rem"
					},
					"size": "3rem",
					"slug": "xx-large"
				},
				{
					"fluid": {
						"min": "3.25rem",
						"max": "4rem"
					},
					"size": "4rem",
					"slug": "huge"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/paragraph": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--custom-beige)",
			"text": "var(--wp--preset--color--black)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--huge)",
					"fontStyle": "normal",
					"fontWeight": "700"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"textTransform": "uppercase"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-small)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--custom-grey)"
				}
			}
		},
		"spacing": {
			"blockGap": "1.56rem",
			"padding": {
				"bottom": "0px",
				"left": "20px",
				"right": "20px",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system-font)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "1.5"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 2,
	"$schema": "https://schemas.wp.org/wp/6.5/theme.json"
}