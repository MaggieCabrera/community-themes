{
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#f2f2f2",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#1da1f2",
					"name": "Base / 2",
					"slug": "base-2"
				},
				{
					"color": "#000035",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#aac9f0",
					"name": "Contrast / 2",
					"slug": "contrast-2"
				},
				{
					"color": "#657786",
					"name": "Contrast / 3",
					"slug": "contrast-3"
				},
				{
					"color": "#aab8c2",
					"name": "Accent",
					"slug": "accent"
				},
				{
					"color": "#14171a",
					"name": "Accent / 2",
					"slug": "accent-2"
				}
			]
		},
		"layout": {
			"contentSize": "620px",
			"wideSize": "1000px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/button": {
				"color": {
					"background": "var(--wp--preset--color--base-2)",
					"text": "var(--wp--preset--color--base)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--base)"
						}
					}
				},
				"typography": {
					"textTransform": "none"
				},
				"variations": {
					"outline": {
						"border": {
							"bottom": {
								"color": "var(--wp--preset--color--base-2)",
								"style": "solid"
							},
							"left": {
								"color": "var(--wp--preset--color--base-2)",
								"style": "solid"
							},
							"right": {
								"color": "var(--wp--preset--color--base-2)",
								"style": "solid"
							},
							"top": {
								"color": "var(--wp--preset--color--base-2)",
								"style": "solid"
							}
						},
						"color": {
							"background": "var(--wp--preset--color--base)",
							"text": "var(--wp--preset--color--contrast)"
						}
					}
				}
			},
			"core/navigation": {
				"typography": {
					"textDecoration": "none"
				}
			},
			"core/navigation-link": {
				"typography": {
					"textDecoration": "underline"
				}
			},
			"core/search": {
				"border": {
					"bottom": {
						"style": "none",
						"width": "0px"
					},
					"left": {
						"style": "none",
						"width": "0px"
					},
					"radius": "0px",
					"right": {
						"style": "none",
						"width": "0px"
					},
					"top": {
						"style": "none",
						"width": "0px"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "2.2"
				}
			},
			"core/separator": {
				"color": {
					"background": "var(--wp--preset--color--contrast-2)"
				},
				"variations": {
					"wide": {
						"color": {
							"background": "var(--wp--preset--color--contrast-2)"
						}
					}
				}
			},
			"core/site-title": {
				"typography": {
					"textDecoration": "none"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"caption": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				}
			},
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--accent-2)"
				},
				"typography": {
					"fontStyle": "normal",
					"fontWeight": "300"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--contrast-3)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--base-2)"
				},
				"typography": {
					"textDecoration": "none"
				}
			},
			"button": {
				":active": {
					"color": {
						"background": "var(--wp--preset--color--contrast)",
						"text": "var(--wp--preset--color--base)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--preset--color--contrast-2)",
						"text": "var(--wp--preset--color--base)"
					},
					"outline": {
						"color": "var(--wp--preset--color--contrast)",
						"offset": "2px"
					},
					"border": {
						"color": "var(--wp--preset--color--contrast-2)"
					}
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--contrast-2)",
						"text": "var(--wp--preset--color--base)"
					},
					"border": {
						"color": "var(--wp--preset--color--contrast-2)"
					}
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system-font)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 2,
	"$schema": "https://schemas.wp.org/wp/6.5/theme.json"
}